snippet main
int main(int argc; const char* argv[])
{
	$1
	return 0;
}
endsnippet


# conditions
snippet if
if($1)
{
	$2
}
endsnippet

# loops 
snippet while
while($1)
{
	$2
}
endsnippet

snippet switch
switch($1)
{
	case $2:
		break;
	
	default:
		break;
}
endsnippet

snippet inc
#include <${1}.h>
endsnippet

snippet func
${1:void} ${2:func}($2)
{
	$3
}
endsnippet
